def dimensionar_transformador(P, Vp, Vs, f):
  # Importando as bibliotecas necessárias
  import numpy as np
  import pandas as pd
  import matplotlib.pyplot as plt

  # Definindo o rendimento do transformador
  eta = 0.9 # Rendimento

  # Calculando as correntes no primário e no secundário
  Ip = P / (Vp * eta)
  Is = P / (Vs * eta)

  # Calculando os números de espiras no primário e no secundário
  Np = Vp / (f * A)
  Ns = Vs / (f * A)

  # Calculando os diâmetros do núcleo e das bobinas
  Dn = Np * d / 1000 # Diâmetro do núcleo em m
  Db = Ns * d / 1000 # Diâmetro das bobinas em m

  # Calculando as áreas das seções transversais do núcleo e das bobinas
  A_nuclero = np.pi * (Dn / 2)**2 # Área da seção transversal do núcleo em m^2
  A_bobina1 = np.pi * (Db / 2)**2 # Área da seção transversal da bobina primária em m^2
  A_bobina2 = np.pi * (Db / 2)**2 # Área da seção transversal da bobina secundária em m^2

  # Calculando as densidades dos materiais usados no núcleo e nas bobinas
  rho_nuclero = 7850 # Densidade do material usado no núcleo [kg/m^3]
  rho_bobina1 = rho_nuclero + rho_bobina2 # Densidade média dos materiais usados nas bobinas [kg/m^3]
  rho_bobina2 = rho_nuclero - rho_bobina1 # Densidade média dos materiais usados nas bobinas [kg/m^3]

  # Calculando os pesos do núcleo e das bobinas
  Wn = Dn * rho_nuclero * A_nuclero * g # Peso do núcleo em kg
  Wb1 = Db * rho_bobina1 * A_bobina1 * g # Peso da bobina primária em kg
  Wb2 = Db * rho_bobina2 * A_bobina2 * g # Peso da bobina secundária em kg

  # Calculando as quantidades de lâminas do núcleo e das bobinas
  Qn = A_nuclero / d # Quantidade de lâminas do núcleo
  Qb1 = A_bobina1 / d # Quantidade de lâminas da bobina primária
  Qb2 = A_bobina2 / d # Quantidade de lâminas da bobina secundária

  # Criando um dicionário com as dimensões do transformador
  dimensoes = {
    "diâmetro do núcleo": Dn,
    "espessura das lâminas": d,
    "área da seção transversal do núcleo": A_nuclero,
    "área da seção transversal da bobina primária": A_bobina1,
    "área da seção transversal da bobina secundária": A_bobina2,
    "número de espiras no primário": Np,
    "número de espiras no secundário": Ns,
    "peso do núcleo": Wn,
    "peso da bobina primária": Wb1,
    "peso da bobina secundária": Wb2,
    "quantidade de lâminas do núcleo": Qn,
    "quantidade de lâminas da bobina primária": Qb1,
    "quantidade de lâminas da bobina secundária": Qb2
  }

  # Retornando o dicionário com as dimensões do transformador
  return dimensoes
