<CorrenteMagnetizacao@Screen>:
    name: "correnteMagnetizacao"
    ScreenManager:
        id: screen_manager
        MDScreen:
            name: "correnteMagnetizacao"
            MDRoundFlatIconButton:
                text: "Importar arquivo"
                icon: "folder"
                pos_hint: {"center_x": .5, "center_y": .2}
                on_release: app.file_manager_open()

            MDRaisedButton:
                text: "Gerar gráfico"
                name: "graph"
                pos_hint: {"center_x":.5, "center_y":.11}
                on_release: app.change_screen("graph")
        
        MDScreen:
            name: "graph"
            BoxLayout:
                id:graph
                orientation: 'vertical'
                pos_hint: {"top":1}
                size_hint_y:.8

            MDRaisedButton:
                text: "Voltar"
                pos_hint: {"center_y":.1, "center_x":.5}
                on_release: app.change_screen("correnteMagnetizacao")



    MDNavigationLayout:
        MDScreenManager:
            MDScreen:
                MDTopAppBar:
                    id: menu_top_app_bar

                    title: "Corrente Magnetização"
                    pos_hint: {"top": 1}
                    left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]

        MDNavigationDrawer:
            id: nav_drawer
            radius: (0, 16, 16, 0)

            MDNavigationDrawerMenu:

                MDNavigationDrawerHeader:
                    title: "Transformadores"
                    title_color: "#4a4939"
                    text: "Projeto UPE-POLI 2023.2"
                    spacing: "4dp"
                    padding: "12dp", 0, 0, "56dp"

                MDNavigationDrawerLabel:
                    text: "Dados Entrada"

                DrawerClickableItem:
                    nav_drawer: nav_drawer
                    icon: "home"
                    text: "Tela Inicial"

                    on_press:
                        nav_drawer.set_state("close")
                        app.root.current = "telaInicial"

                MDNavigationDrawerDivider:

                MDNavigationDrawerLabel:
                    text: "Desenvolvimento"

                DrawerClickableItem:
                    nav_drawer: nav_drawer
                    icon: "note-edit"
                    text: "Dimensionamento"

                    on_press:
                        nav_drawer.set_state("close")
                        app.root.current = "dimensionamento"

                DrawerClickableItem:
                    icon: "sine-wave"
                    text: "Corrente Magnetização"

                    on_press:
                        nav_drawer.set_state("close")
                        app.root.current = "correnteMagnetizacao"

                MDNavigationDrawerDivider:

                MDNavigationDrawerLabel:
                    text: "Detalhamento"

                DrawerClickableItem:
                    icon: "file-document"
                    text: "Ficha Técnica"

                    on_press:
                        nav_drawer.set_state("close")
                        app.root.current = "fichaTecnica"

                DrawerClickableItem:
                    icon: "account-group"
                    text: "Desenvolvedores"

                    on_press:
                        nav_drawer.set_state("close")
                        app.root.current = "desenvolvedores"
